.stageUnlock {
}

.stageUnlock #containerStageUnlock{
    width: 300px;
    height: 226px;
    top: auto;
    align-self: center;

    background-image: resource("panel");
}

.stageUnlock #textStageUnlock{
    color: rgba(0,0,0,1);
    font-size: 90%;
    letter-spacing: 5%;
    top: 20px;
    left: 15px;
}

.stageUnlock #buttonStageUnlockAccept{
    width: 90px;
    height: 90px;
    left: 100px;
    top: 91px;
    align-self: center;

    background-image: resource("correct");
}

.stageUnlock #buttonStageUnlockDeny{
    width: 90px;
    height: 90px;
    left: -100px;
    top: 1px;
    align-self: center;

    background-image: resource("wrong");
}

.hide{
    display: none;
}

